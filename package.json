{
  "name": "trav",
  "version": "0.1.0",
  "author": "Joe-noh",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Joe-noh/trav-front.git"
  },
  "homepage": "https://github.com/Joe-noh/trav-front#readme",
  "bugs": {
    "url": "https://github.com/Joe-noh/trav-front/issues"
  },
  "angular-cli": {},
  "scripts": {
    "start": "npm run server:dev",
    "server:dev": "NODE_ENV=dev webpack-dev-server --inline --progress --profile --colors --watch --display-error-details --display-cached --content-base src/client/",
    "test:unit": "karma start config/karma.conf.js",
    "test:unit:watch": "npm run test:unit -- --auto-watch --no-single-run",
    "test:e2e": "protractor config/protractor.conf.js",
    "test:e2e:prepare": "webdriver-manager update --standalone && webdriver-manager start"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^2.0.0-rc.1",
    "@angular/compiler": "^2.0.0-rc.1",
    "@angular/core": "^2.0.0-rc.1",
    "@angular/http": "^2.0.0-rc.1",
    "@angular/platform-browser": "^2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "^2.0.0-rc.1",
    "@angular/router": "^0.2.0",
    "@angular/router-deprecated": "^2.0.0-rc.1",
    "core-js": "^2.3.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "^5.0.0-beta.6",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^0.17.0",
    "copy-webpack-plugin": "^2.1.3",
    "exports-loader": "^0.6.3",
    "html-webpack-plugin": "^2.16.1",
    "jasmine-core": "^2.3.4",
    "jasmine-spec-reporter": "^2.4.0",
    "karma": "^0.13.15",
    "karma-chrome-launcher": "^0.2.1",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "phantomjs-polyfill": "0.0.2",
    "phantomjs-prebuilt": "^2.1.7",
    "protractor": "^3.3.0",
    "raw-loader": "^0.5.1",
    "source-map-loader": "^0.1.5",
    "ts-helpers": "^1.1.1",
    "ts-node": "^0.7.2",
    "typescript": "^1.8.7",
    "typings": "^0.7.12",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.12.0"
  }
